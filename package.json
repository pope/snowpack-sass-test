{
  "name": "snowpack-test",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "snowpack build",
    "format": "prettier --write '{public,src,types}/**/*.{ts,tsx,scss,html,js,json}' '*.{js,json}'",
    "start": "snowpack dev"
  },
  "devDependencies": {
    "@snowpack/plugin-sass": "^1.3.0",
    "@snowpack/plugin-typescript": "^1.2.1",
    "import-sort-style-module": "^6.0.0",
    "prettier": "^2.2.1",
    "prettier-plugin-import-sort": "^0.0.6",
    "sass": "^1.32.5",
    "snowpack": "^3.0.11",
    "typescript": "^4.1.3"
  },
  "prettier": {
    "trailingComma": "es5",
    "singleQuote": true
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "dependencies": {
    "@material/theme": "^9.0.0",
    "@material/typography": "^9.0.0"
  }
}
